{
  "hosting": {
    "public": "./",
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    }, {
      "source": "/resume",
      "destination": "/resume.html"
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/scss/**",
      "*.md"
    ]
  }
}
